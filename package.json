{
	"name": "ppro-ext-cep-typescript-react",
	"version": "1.0.0",
	"description": "Template for ppro-ext-cep-typescript-react.",
	"main": "client/src/index.html",
	"scripts": {
		"build-dev": "npm run clean && npm run webpack-client -- --mode=development && npm run build-host && npm run copy-csxs && npm run copy-debug-file",
		"build-prod": "npm run clean && npm run webpack-client -- --mode=production  && npm run build-host && npm run copy-csxs",
		"webpack-client": "webpack --config webpack.config.client.js",
		"build-host": "tsc -p ./host/tsconfig.json",
		"copy-csxs": "cp -r ./CSXS/ ./dist/",
		"copy-debug-file": "cp -r ./.debug ./dist/",
		"clean": "rm -rf ./dist"
	},
	"keywords": [
		"typescript",
		"react",
		"Adobe",
		"Premiere Pro",
		"Extension"
	],
	"author": "LiuToki",
	"license": "MIT",
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.5",
		"@babel/core": "^7.14.6",
		"@babel/preset-env": "^7.14.7",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.14.5",
		"@types/react": "^17.0.14",
		"@types/react-dom": "^17.0.9",
		"babel-loader": "^8.2.2",
		"html-webpack-plugin": "^5.3.2",
		"ts-csinterface": "^1.0.2",
		"types-for-adobe": "github:pravdomil/types-for-adobe",
		"typescript": "^4.3.5",
		"webpack": "^5.44.0",
		"webpack-cli": "^4.7.2",
		"webpack-dev-server": "^3.11.2"
	}
}
